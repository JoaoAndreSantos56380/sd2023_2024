    "version": "0.2.0",
    "configurations": [
        {
            "name": "Data",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/binary/test_data",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            //"miDebuggerPath": "C:\MinGW\bin\gdb.exe",
            //"miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "Compile",
            "setupCommands": [
                {
                    "description": "Enable funcy printing to gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "In this mode GDB will be attached to both processes after a call to fork() or vfork().",
                    "text": "-gdb-set detach-on-fork on",
                    "ignoreFailures": false
                },
                {
                    "description": "The new process is debugged after a fork. The parent process runs unimpeded.",
                    "text": "-gdb-set follow-fork-mode child",
                    "ignoreFailures": false
                }
            ]
        },
